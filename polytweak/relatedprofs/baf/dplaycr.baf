IF
	Global("a^backswordprof","LOCALS",0)
	CheckStatGT(Myself,1,PROFICIENCYSCIMITARWAKISASHININJATO)
	!CheckStatGT(Myself,0,PROFICIENCYKATANA)
	!Class(Myself,DRUID_ALL)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
		SetGlobal("a^backswordprof","LOCALS",1)
		ApplySpellRES("A^KATPR1",Myself) // No such index
		SetInterrupt(TRUE)
END

